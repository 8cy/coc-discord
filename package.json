{
  "name": "coc-discord",
  "version": "0.0.6",
  "description": "an improved discord rich presence for coc.nvim",
  "main": "lib/index.js",
  "scripts": {
    "clean": "rimraf lib/",
    "build": "tsc -p tsconfig.json",
    "watch": "tsc -w -p tsconfig.json",
    "lint": "eslint src/*",
    "copy:config": "copyfiles -f src/config.json lib/",
    "prepack": "yarn clean && yarn build && yarn copy:config"
  },
  "keywords": [
    "coc.nvim",
    "vim",
    "discord"
  ],
  "homepage": "https://github.com/8cy/coc-discord#readme",
  "repository": "github:8cy/coc-discord",
  "license": "MIT",
  "engines": {
    "coc": ">=0.0.70"
  },
  "devDependencies": {
    "@types/discord-rpc": "^3.0.2",
    "@types/node": "^13.9.1",
    "@typescript-eslint/eslint-plugin": "^2.26.0",
    "@typescript-eslint/parser": "^2.26.0",
    "coc.nvim": "^0.0.76",
    "copyfiles": "^2.3.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-typescript": "^7.2.0",
    "eslint-plugin-import": "^2.20.1",
    "rimraf": "^3.0.2",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "discord-rpc": "^3.1.0",
    "fp-ts": "^2.5.3"
  },
  "author": {
    "name": "Sin"
  }
}
